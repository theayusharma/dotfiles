"custom/microphone": {
    "exec": "pactl get-source-mute @DEFAULT_SOURCE@ | grep -q 'yes' && echo '<span color=\"#f38ba8\"> ï„± </span>' || echo '<span color=\"#a6e3a1\"> ï„°</span>'",
    "return-type": "text",
    "interval": 30,
    "format": "{}",
    "on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
    "on-scroll-up": "pactl set-source-volume @DEFAULT_SOURCE@ +5%",
    "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -5%",
    "tooltip": false
},
    "custom/bluetooth": {
        "format": "<span color=\"#89b4fa\">ó°‚¯  </span>",
        "interval": 30,
        "rotate": 0,
        "on-click": "blueberry",
        "tooltip": false
    },
   


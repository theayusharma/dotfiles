    "tooltip": false,
    "format": "{icon}  ",
    "format-icons": {
        "notification": "<span color='#f9e2af'>ï‚¢</span><span foreground='#f38ba8'><sup>ï‘„  </sup></span>",               // Yellow bell + Red dot
        "none": "<span color='#f9e2af'>ï‚¢</span>",                                                                  // Yellow bell
